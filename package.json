{
  "name": "app-shell",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.9.0",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "office-ui-fabric-react": "^5.98.0",
    "react": "^16.3.2",
    "react-content-loader": "^3.1.1",
    "react-dom": "^16.3.2",
    "react-i18nify": "^1.11.14",
    "react-router-dom": "^4.2.2",
    "react-winjs": "git+https://github.com/TECLIB/react-winjs.git#develop",
    "winjs": "^4.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-styles && yarn lint && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prestart": "yarn build-styles && node-sass --watch src/assets/styles/scss/index.scss src/assets/styles/css/index.css &",
    "build-styles": "node-sass src/assets/styles/scss/index.scss src/assets/styles/css/index.css",
    "lint": "yarn eslint src"
  }
