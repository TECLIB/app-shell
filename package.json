{
  "name": "app-shell",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "node-sass": "^4.9.2",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "node-polyglot": "^2.3.0",
    "office-ui-fabric-react": "^6.42.0",
    "react": "^16.4.1",
    "react-content-loader": "^3.1.2",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-winjs": "git+https://github.com/TECLIB/react-winjs.git#develop",
    "winjs": "^4.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-styles && yarn lint && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prestart": "yarn build-styles && node-sass --watch src/assets/styles/scss/index.scss src/assets/styles/css/index.css &",
    "build-styles": "node-sass src/assets/styles/scss/index.scss src/assets/styles/css/index.css",
    "lint": "yarn eslint src"
  }
}
